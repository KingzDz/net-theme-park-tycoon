<UserControl x:Class="ThemeParkTycoonGame.Fancy.Controls.MarketControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ThemeParkTycoonGame.Fancy.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <StackPanel Width="650" Height="450">
            <StackPanel x:Name="ShopList">
                <TextBlock x:Name="Testblock1"></TextBlock>
                <TextBlock x:Name="Testblock2"></TextBlock>
                <TextBlock x:Name="Testblock3"></TextBlock>
                <TextBlock x:Name="Testblock4"></TextBlock>
                <TextBlock x:Name="Testblock5"></TextBlock>
                <Button x:Name="MarketButton" Click="MarketButton_Click"></Button>
            </StackPanel>
        </StackPanel>
        <StackPanel Width="150" Height="450">
            
        </StackPanel>
    </StackPanel>
</UserControl>
