<Window x:Class="ThemeParkTycoonGame.Fancy.Controls.weatherChange"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThemeParkTycoonGame.Fancy.Controls"
        mc:Ignorable="d"
        Title="weatherChange" Height="190" Width="400">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Image Height="50" Width="50" Source="Recources/weather_snow_48.png"></Image>
            <Button x:Name="weatherChangeButton" Content="weather change" Height="20" Width="100" Click="WeatherChangeButton_Click">
            </Button>
        </StackPanel>
    </Grid>
</Window>
